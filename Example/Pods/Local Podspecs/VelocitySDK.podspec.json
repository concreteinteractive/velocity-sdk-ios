{
  "name": "VelocitySDK",
  "version": "1.2.2",
  "summary": "The open source library for Velocity SDK.",
  "homepage": "https://www.vlcty.net/",
  "license": "MIT",
  "authors": {
    "Concrete Interactive": "start@concreteinteractive.com"
  },
  "source": {
    "git": "https://github.com/concreteinteractive/velocity-sdk-ios.git",
    "tag": "1.2.2",
    "submodules": true
  },
  "platforms": {
    "ios": "8.0"
  },
  "requires_arc": true,
  "public_header_files": [
    "Framework/VelocitySDK.h",
    "VelocitySDK/VLTErrors.h",
    "VelocitySDK/VLTManager.h",
    "VelocitySDK/VLTGPS.h",
    "VelocitySDK/Core/VLTMotionDetectResult.h"
  ],
  "source_files": [
    "Framework/VelocitySDK.h",
    "VelocitySDK/**/*.{h,m}"
  ],
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GPB_USE_PROTOBUF_FRAMEWORK_IMPORTS=1"
  },
  "frameworks": [
    "UIKit",
    "CoreData"
  ],
  "module_name": "VelocitySDK",
  "dependencies": {
    "AFNetworking/NSURLSession": [

    ],
    "Protobuf": [
      "~> 3.4.0"
    ],
    "SocketRocket": [
      "~> 0.6.0"
    ]
  },
  "subspecs": [
    {
      "name": "no-arc",
      "requires_arc": false,
      "source_files": "vendor/velocity-protobuf/lib/objc/*.{h,m}"
    }
  ]
}
